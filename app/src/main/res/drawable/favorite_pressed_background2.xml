<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="oval">
            <size
                android:height="16dp"
                android:width="16dp" />
            <solid android:color="@color/colorFavorite" />
        </shape>
    </item>
    <item
        android:bottom="1dp"
        android:left="1dp"
        android:right="1dp"
        android:top="1dp">
        <shape android:shape="oval">
            <size
                android:width="16dp"
                android:height="16dp" />
            <solid android:color="@color/colorWhite" />
        </shape>
    </item>
    <item
        android:bottom="3dp"
        android:left="3dp"
        android:right="3dp"
        android:top="3dp">
        <shape android:shape="oval">
            <size
                android:width="16dp"
                android:height="16dp" />
            <solid android:color="@color/colorFavorite" />
        </shape>
    </item>
</layer-list>