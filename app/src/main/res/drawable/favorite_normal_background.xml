<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="oval">
            <size
                android:height="16dp"
                android:width="16dp" />
            <solid android:color="@color/colorFavorite" />
        </shape>
    </item>
    <item
        android:bottom="2dp"
        android:left="2dp"
        android:right="2dp"
        android:top="2dp">
        <shape android:shape="oval">
            <size
                android:width="16dp"
                android:height="16dp" />
            <solid android:color="@color/colorWhite" />
        </shape>
    </item>
</layer-list>