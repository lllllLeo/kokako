<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="oval">
            <size
                android:height="12dp"
                android:width="12dp" />
            <solid android:color="@color/colorPrimaryDark" />
        </shape>
    </item>
    <item
        android:bottom="2dp"
        android:left="2dp"
        android:right="2dp"
        android:top="2dp">
        <shape android:shape="oval">
            <size
                android:width="12dp"
                android:height="12dp" />
            <solid android:color="@color/colorPrimary" />
        </shape>
    </item>
    <item
        android:bottom="2dp"
        android:left="2dp"
        android:right="2dp"
        android:top="2dp"
        android:drawable="@drawable/ic_baseline_check_24"
        android:width="12dp"
        android:height="12dp" />
</layer-list>